{
  "name": "botfront",
  "version": "0.17.2",
  "private": true,
  "repository": "https://github.com/botfront/botfront",
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "meteor run",
    "start:docker-compose": "BF_PROJECT_ID=bf ORCHESTRATOR=docker-compose meteor run",
    "start:docker-compose.dev": "NODE_ENV=development BF_PROJECT_ID=bf MONGO_URL=mongodb://localhost:27017/bf ORCHESTRATOR=docker-compose meteor run",
    "start:docker-compose.ci": "NODE_ENV=test BF_PROJECT_ID=bf MONGO_URL=mongodb://localhost:27017/bf ORCHESTRATOR=docker-compose meteor run",
    "test-watch": "TEST_BROWSER_DRIVER=chrome meteor test --driver-package meteortesting:mocha --port 3100",
    "test-once": "TEST_BROWSER_DRIVER=chrome meteor test --once --driver-package meteortesting:mocha --port 3100",
    "debug": "meteor run --inspect-brk=9229",
    "test:e2e": "CYPRESS_RETRIES=2 npx cypress run",
    "ci": "start-server-and-test start http://localhost:3000 test:e2e",
    "docs:dev": "npx vuepress dev docs",
    "docs:build": "npx vuepress build docs",
    "licenses:summary": "license-checker --summary",
    "licenses:csv": "license-checker --csv --out botfront-licenses.csv",
    "release": "standard-version",
    "storybook": "start-storybook",
    "postinstall": "cp -R node_modules/semantic-ui-css/themes/default/assets/fonts ./public/ && cp -R node_modules/semantic-ui-css/themes/default/assets/fonts ./client/"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "axios": "^0.19.0",
    "axios-retry": "^3.1.2",
    "brace": "^0.11.1",
    "buffer": "^5.2.1",
    "dotenv": "^7.0.0",
    "emoji-tree": "^1.1.2",
    "fast-crc32c": "^1.0.4",
    "file-saver": "^2.0.2",
    "immutable": "^4.0.0-rc.12",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "match-sorter": "^4.0.0",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "prop-types": "^15.7.2",
    "query-string": "^5.1.0",
    "random-seed": "^0.3.0",
    "rasa-webchat": "^0.6.5",
    "react": "^16.8.6",
    "react-ace": "^6.1.4",
    "react-debounce-input": "^3.2.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-dropzone": "^4.3.0",
    "react-google-recaptcha": "^1.0.5",
    "react-intercom": "^1.0.15",
    "react-json-view": "^1.19.1",
    "react-redux": "^5.1.0",
    "react-required-if": "^1.0.3",
    "react-router": "^3.2.1",
    "react-s-alert": "^1.4.1",
    "react-scrollbar": "^0.5.6",
    "react-select": "^1.2.1",
    "react-split-pane": "^0.1.87",
    "react-syntax-highlighter": "^8.0.1",
    "react-table": "^6.8.6",
    "react-transition-group": "^2.4.0",
    "react-window-size": "^1.2.2",
    "redux": "^4.0.4",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.86.0",
    "shortid": "^2.2.8",
    "simpl-schema": "^1.5.5",
    "slugify": "^1.3.4",
    "standard-version": "^6.0.1",
    "uniforms": "^1.31.1",
    "uniforms-semantic": "^1.31.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@vuepress/plugin-active-header-links": "^1.2.0",
    "@vuepress/plugin-back-to-top": "^1.1.0",
    "@vuepress/plugin-google-analytics": "^1.2.0",
    "@vuepress/plugin-medium-zoom": "^1.2.0",
    "@vuepress/plugin-pwa": "^1.2.0",
    "autoprefixer": "^6.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "chai": "^4.1.2",
    "chromedriver": "^78.0.1",
    "commitizen": "^3.0.7",
    "commitlint": "8.1.0",
    "core-js": "^2.5.3",
    "cypress": "^3.4.1",
    "cypress-plugin-retries": "^1.2.2",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-meteor": "^5.1.0",
    "eslint-plugin-react": "^7.11.1",
    "faker": "^4.1.0",
    "husky": "2.7.0",
    "less": "2.5.0",
    "less-loader": "^5.0.0",
    "license-checker": "^25.0.1",
    "selenium-webdriver": "3.6.0",
    "start-server-and-test": "^1.9.1",
    "vuepress": "^1.1.0",
    "webpack": "^4.28.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "standard-version": {
    "scripts": {
      "postcommit": "../scripts/post_commit.sh"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/commitlint"
    }
  }
}
